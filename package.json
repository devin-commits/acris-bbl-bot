{
  "name": "acris-bbl-bot",
  "version": "0.1.0",
  "description": "CLI tool that navigates NYC ACRIS by BBL and summarizes key development rights transactions",
  "private": true,
  "main": "dist/index.js",
  "bin": {
    "acris-bbl-bot": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "scrape": "tsx src/index.ts --trace",
    "scrape:headful": "tsx src/index.ts --headful --trace",
    "scrape:batch": "tsx scripts/run-batch.ts",
    "dev": "tsx src/index.ts --headful --trace",
    "start": "tsx src/index.ts --headful --trace",
    "watch": "tsx src/index.ts --headful --trace --bbl 1009180051",
    "gemini:extract": "tsx scripts/gemini-extract.ts",
    "gemini:aggregate": "tsx scripts/gemini-aggregate.ts",
    "api": "tsx src/server.ts",
    "test": "vitest --run --reporter=verbose --bail=1 --passWithNoTests",
    "test:e2e": "vitest --run --reporter=verbose e2e",
    "ocr": "tsx ./scripts/ocr-pass.ts",
    "check:github": "node check-github-auth.js",
    "copy:zip": "node copy-zip.js"
  },
  "keywords": [
    "acris",
    "nyc",
    "bbl",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "-": "^0.0.1",
    "@google/genai": "^1.30.0",
    "@google/generative-ai": "^0.24.1",
    "@napi-rs/canvas": "^0.1.51",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "g": "github:openai/codex",
    "node-html-parser": "^6.1.13",
    "pdf-parse": "^1.1.3",
    "pdfjs-dist": "^5.4.296",
    "playwright": "^1.43.0",
    "puppeteer": "^24.31.0",
    "sharp": "^0.34.4",
    "tesseract.js": "^4.1.4",
    "zod": "^3.23.8"
  },
  "overrides": {},
  "devDependencies": {
    "@types/node": "^20.19.24",
    "@types/pdf-parse": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "tsx": "^4.20.6",
    "typescript": "^5.4.2",
    "vitest": "^4.0.4"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  }
}
